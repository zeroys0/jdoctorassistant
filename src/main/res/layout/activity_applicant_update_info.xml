        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            >
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="@string/professional"
                android:layout_margin="10dp"
                />

            <TextView
                android:id="@+id/community_pprofessional"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="杨健"
                android:layout_margin="10dp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            >
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="@string/native_place"
                android:layout_margin="10dp"
                />

            <TextView
                android:id="@+id/community_pnative_place"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="天津"
                android:layout_margin="10dp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:layout_marginTop="1dp"
            >
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:text="@string/is_married"
                android:layout_margin="10dp"
                />

            <TextView
                android:id="@+id/community_pmarita"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="杨健"
                android:layout_margin="10dp"
                />
        </LinearLayout>

    </LinearLayout>
</LinearLayout>
